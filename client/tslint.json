{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "jsx-boolean-value": ["never"],
    "interface-name": false,
    "no-console": false,
    "member-access": [true, "no-public"],
    "object-literal-sort-keys": false
  }
}
